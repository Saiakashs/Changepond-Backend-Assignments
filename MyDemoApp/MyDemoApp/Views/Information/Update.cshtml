
@{
    ViewData["Title"] = "Update Information";
}

@model Information
<a asp-action="Index" class="btn btn-secondary" style="margin-bottom:10px">Go Back</a>
<h1 class="bg-info text-white">Update Information</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="License"></label>
        <input asp-for="License" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Established"></label>
        <input asp-for="Established" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Revenue"></label>
        <input asp-for="Revenue" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary" style="margin-top:10px">Update</button>
</form>

